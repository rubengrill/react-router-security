{
  "name": "react-router-security",
  "version": "2.0.0",
  "description": "Secure routes of React Router",
  "files": [
    "*.md",
    "docs",
    "lib",
    "npm-scripts",
    "umd"
  ],
  "main": "lib/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/rubengrill/react-router-security.git"
  },
  "scripts": {
    "build": "rimraf lib && babel ./modules -d lib --ignore '__tests__'",
    "build-umd": "NODE_ENV=production webpack modules/index.js umd/ReactRouterSecurity.js",
    "build-min": "NODE_ENV=production webpack -p modules/index.js umd/ReactRouterSecurity.min.js",
    "lint": "eslint modules examples",
    "test": "npm run lint && karma start",
    "start": "node examples/server.js",
    "postinstall": "node ./npm-scripts/postinstall.js"
  },
  "authors": [
    "Ruben Grill"
  ],
  "license": "MIT",
  "peerDependencies": {
    "react-router": "^2.0.0"
  },
  "devDependencies": {
    "babel": "^5.8.34",
    "babel-core": "^5.8.34",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^5.4.0",
    "body-parser": "^1.14.1",
    "eslint": "^1.10.3",
    "eslint-config-rackt": "^1.1.0",
    "eslint-plugin-react": "^3.11.3",
    "express": "^4.13.3",
    "express-session": "^1.12.1",
    "express-urlrewrite": "^1.2.0",
    "gzip-size": "^3.0.0",
    "isomorphic-fetch": "^2.2.0",
    "karma": "^0.13.15",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.3",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "phantomjs": "^1.9.18",
    "pretty-bytes": "^2.0.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-router": "^2.0.0",
    "redux": "^3.0.4",
    "rimraf": "^2.4.3",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.4.0"
  },
  "tags": [
    "react",
    "router",
    "security"
  ],
  "keywords": [
